    <script>
      Holder.addTheme('thumb', {
        bg: '#55595c',
        fg: '#eceeef',
        text: 'Thumbnail'
      });
    </script>
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-138130371-1"></script>
